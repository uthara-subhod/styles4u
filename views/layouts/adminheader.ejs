        </button>
  
        <!-- Brand -->
        <a class="navbar-brand" href="#">
                <h2 >Styles4U<span class="text-white px-1 mr-1"><small>admin</small></span></h2>
        </a>
        <!-- Right links -->
        <ul class="navbar-nav ms-auto d-flex flex-row text-white">
            <li class="px-2 text-white">admin</li> /
            <li class="px-2"><a href="/admin/logout" class="text-white">Logout</a></li>
        </ul>
      </div>
      <!-- Container wrapper -->
    </nav>
    <!-- Navbar -->
  </header>
